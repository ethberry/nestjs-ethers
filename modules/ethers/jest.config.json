{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"],
  "coverageDirectory": "../coverage",
  "moduleFileExtensions": ["js", "jsx", "ts", "tsx", "json"],
  "preset": "ts-jest",
  "rootDir": "src",
  "testEnvironment": "node",
  "testMatch": ["**/*.spec.ts"],
  "verbose": true
}
